    "concurrently": "^4.1.0",
    "create-react-app": "^2.1.3",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.4.4",
    "nodemon": "^1.18.9",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "redux": "^4.0.1",
    "terser": "^3.14.1",
    "validator": "^10.11.0"
  }
